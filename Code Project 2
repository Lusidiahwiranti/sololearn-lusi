import java.util.Scanner;

public class Program
{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int amount = scanner.nextInt();
		int duetku=0;
		//your code goes here
		for (int i=0; i<3; i++){
			duetku = amount/10;
			amount-= duetku;
		}
		System.out.println(amount);
	}
}
